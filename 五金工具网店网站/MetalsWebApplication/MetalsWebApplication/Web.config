<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.6" />
      <httpRuntime targetFramework="4.6" />
    </system.web>
  
  <appSettings>
    <add key="pagecount" value="9" />
  </appSettings>

  <system.webServer>
    <defaultDocument>
      <files>
        <clear/>
        <add value="index.ashx"/>
      </files>
    </defaultDocument>
  </system.webServer>
  
  <connectionStrings>
    <add name="connstr" connectionString="Data Source=.;Initial Catalog=metalsweb;User Id=sa;Password=123456;"></add>
  </connectionStrings>

</configuration>
